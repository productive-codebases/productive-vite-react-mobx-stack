{
  "name": "productive-vite-react-mobx-stack",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    ":d": "npm run start:dev",
    ":dd": "cross-env ENVIRONMENT=mocks concurrently \"npm run :d\" \"npm run :m\"",
    ":m": "npm run start:all --prefix mocks",
    "build:client:prod": "npm run build:prod --prefix client",
    "build:prod": "npm run build:server:prod && npm run build:client:prod",
    "build:server:prod": "npm run build:prod --prefix server",
    "check:code:client": "npm run check:code --prefix client",
    "check:code:common": "npm run check:code --prefix common",
    "check:code:server": "npm run check:code --prefix server",
    "check:code:tooling": "npm run check:code --prefix tooling",
    "check:code:mocks": "npm run check:code --prefix mocks",
    "check:code": "concurrently -r \"npm run check:code:*\"",
    "format:code:client": "npm run format:code --prefix client",
    "format:code:common": "npm run format:code --prefix common",
    "format:code:server": "npm run format:code --prefix server",
    "format:code:tooling": "npm run format:code --prefix tooling",
    "format:code:mocks": "npm run format:code --prefix mocks",
    "format:code": "concurrently -r \"npm run format:code:*\"",
    "lint:code:client": "npm run lint:code --prefix client",
    "lint:code:common": "npm run lint:code --prefix common",
    "lint:code:server": "npm run lint:code --prefix server",
    "lint:code:tooling": "npm run lint:code --prefix tooling",
    "lint:code:mocks": "npm run lint:code --prefix mocks",
    "lint:code": "concurrently -r \"npm run lint:code:*\"",
    "start:client:dev": "npm run start:dev --prefix client",
    "start:client:local": "npm run start:local --prefix client",
    "start:dev": "concurrently -r \"npm run start:*:dev\"",
    "start:local": "concurrently \"npm run start:server:dev\" \"npm run start:client:local\"",
    "start:prod": "concurrently -r \"npm run start:*:prod\"",
    "start:proxy": "npm run start:proxy --prefix tooling",
    "start:server:dev": "npm run start:dev --prefix server",
    "start:server:prod": "npm run start:prod --prefix server",
    "test:client": "npm run test --prefix client",
    "test:common": "npm run test --prefix common",
    "test": "npm run test:common && npm run test:client"
  },
  "author": "productive-codebases",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  }
}
