{
  "name": "mocks",
  "version": "1.0.0",
  "description": "Mocks for T.ID",
  "scripts": {
    "check:code": "tsc --noEmit",
    "lint:code": "eslint . --ext .ts,.tsx --quiet",
    "generate:mocks": "cross-env DEBUG=mocks:generator:* node -r esbuild-register src/data",
    "start:all": "cross-env DEBUG=* npm run generate:mocks && concurrently -r \"npm run start:*(!all)\"",
    "start:session": "nodemon --config nodemon.session.json",
    "start:serverProperties": "nodemon --config nodemon.serverProperties.json",
    "start:dashboard": "nodemon --config nodemon.dashboard.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "productive-codebases",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.3",
    "@types/uuid": "^8.3.4",
    "bouchon-reloaded": "^2.0.0-dev.18",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.15",
    "esbuild-register": "^3.3.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.19",
    "ts-jest": "^29.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@ngneat/falso": "^6.2.0",
    "bunyan": "^1.8.15",
    "colors": "^1.4.0",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0"
  }
}
